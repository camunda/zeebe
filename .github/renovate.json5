{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    // Base configs for renovate
    "config:best-practices",
    // Sets the automerge type to 'pr'
    ":automergePr",
    // Automerge can only happen when all checks are green
    ":automergeRequireAllStatusChecks",
    // Create separate PRs for patch releases
    ":separatePatchReleases",
    // Default labels added to event renovate PR
    ":labels(dependencies)",
    // Raise PR when vulnerability alerts are detected
    ":enableVulnerabilityAlertsWithLabel(area/security)",
    // Groups together all Typescript deps
    "group:definitelyTyped",
    // Pins docker digests
    "docker:pinDigests",
  ],
  // Commit message prefix
  commitMessagePrefix: "deps:",
  // How many days have to pass from the new package version appearance before the PR is created
  minimumReleaseAge: "3 days",
  // Disable auto rebasing
  rebaseWhen: "never",
  // When renovate should do the checks
  schedule: ["every weekday"],
  // How many PRs can exist at once
  prConcurrentLimit: 60,
  // Branches to be checked
  baseBranches: [
    "/^release\\/.*/",
    "maintenance/3.13",
    "maintenance/3.12",
    "maintenance/3.11",
    "maintenance/3.10",
    "master",
  ],
  // This ignores are the ones coming by default from config:base with exception to **/test/**
  // We do this to be able to test files in optimize/docker/test directory
  ignorePaths: [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**",
    "**/examples/**",
    "**/__tests__/**",
    "**/tests/**",
    "**/__fixtures__/**",
  ],
  // Here are credentials to private repos that renovate could not access by default
  hostRules: [
    {
      matchHost: "https://api.github.com/repos/camunda-cloud/",
      hostType: "github-tags",
      encrypted: {
        token: "",
      },
    },
    {
      matchHost: "https://artifacts.camunda.com/artifactory/internal/",
      hostType: "maven",
      encrypted: {
        username: "",
        password: "",
      },
    },
  ],
  helmv3: {
    registryAliases: {
      "helm-camunda-io": "https://helm.camunda.io",
    },
  },
  packageRules: [
    // Ignore deps
    {
      matchDepNames: [
        "io.camunda.optimize:optimize-data-generator",
        "io.camunda.optimize:camunda-optimize",
        "io.camunda.optimize:util",
        "io.camunda.optimize:qa",
        "io.camunda.optimize:optimize-plugin-parent",
        "org.camunda.bpm.model:camunda-bpmn-model",
        "org.camunda.bpm.model:camunda-dmn-model",
        "org.camunda.bpm:camunda-engine-rest-core",
        "org.camunda.bpm:camunda-engine",
        "org.camunda.bpm:camunda-license-check",
        "io.camunda:identity-sdk",
        "io.camunda:identity-spring-boot-starter",
        "io.camunda:zeebe-client-java",
        "io.camunda:zeebe-protocol",
        "ant-contrib:ant-contrib",
        "old-bpmn-js",
        "react",
        "react-dom",
        "react-router",
        "react-router-dom",
        "@types/react",
        "@types/react-dom",
        "@types/react-router-dom",
      ],
      enabled: false,
    },
    // Frontend deps
    {
      addLabels: ["component:frontend"],
      matchCategories: ["js"],
    },
    // Backend deps
    {
      addLabels: ["component:backend"],
      matchCategories: ["java"],
    },
    // Infra deps
    {
      addLabels: ["component:ci"],
      matchManagers: ["docker-compose", "dockerfile", "github-actions"],
    },
    // Docker deps
    {
      matchManagers: "dockerfile",
      separateMajorMinor: true,
      separateMinorPatch: true,
      prPriority: 100,
    },
    // Settings for patch versions update
    // BE
    {
      matchUpdateTypes: ["patch"],
      matchCategories: ["java"],
      groupName: "Patch dependencies - BE",
    },
    // FE
    {
      matchUpdateTypes: ["patch"],
      matchCategories: ["js"],
      groupName: "Patch dependencies - FE",
    },
    // Disable major and minor updates for maintenance branches
    {
      matchUpdateTypes: ["major", "minor"],
      matchBaseBranches: ["/^maintenance\\/.*/", "/^release\\/.*/"],
      enabled: false,
    },
    // Automerge
    {
      matchUpdateTypes: [
        "patch",
        "pin",
        "pinDigest",
        "digest",
        "lockFileMaintenance",
      ],
      automerge: true,
      addLabels: ["automerge"],
    },
  ],
  platformAutomerge: false,
  customManagers: [
    // These two regex managers are checking for alpine image versions in files other than Dockerfile
    {
      customType: "regex",
      fileMatch: [
        ".github/scripts/build-docker-image.sh",
      ],
      matchStrings: [
        "export BASE_IMAGE=(?<depName>.*?):(?<currentValue>.*?)\\s",
      ],
      datasourceTemplate: "docker",
    },
    {
      customType: "regex",
      fileMatch: [
        "optimize/docker/test/verify.sh",
        "optimize/docker/test/docker-labels.golden.json",
      ],
      matchStrings: [
        "(?<depName>docker\\.io\\/library\\/alpine):(?<currentValue>\\d+\\.\\d+\\.\\d+)",
      ],
      datasourceTemplate: "docker",
    },
  ],
}
